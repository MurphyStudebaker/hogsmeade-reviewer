<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.murphy.restaurantrec">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Hogsmeade"
        android:name=".HogsmeadeApplication"
        android:roundIcon="@drawable/beer_icon"
        android:supportsRtl="true">

        <activity android:name=".RestaurantList">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".AddRestaurant"
            android:theme="@style/Butterbeer"></activity>
        <activity android:name=".RestaurantInfo"
            android:theme="@style/Butterbeer"></activity>
    </application>
</manifest>
